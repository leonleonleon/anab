{
  "name": "anab",
  "version": "1.0.0",
  "description": "",
  "main": "./server/server.js",
  "engines": {
    "node": "8.2.1",
    "npm": "5.3.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "node ./server/server.js",
    "build": "npm run test && NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.prod.js",
    "dev": "webpack-dev-server --progress",
    "test": "npm run test:styles && npm run test:eslint && npm run test:unit",
    "test:unit": "echo \"unit testing\" && NODE_ENV=test jest --coverage --colors ",
    "test:styles": "./node_modules/.bin/stylelint ./app/**/*.css, ./app/**/**/*.css",
    "test:eslint": "echo \"js linting\" && ./node_modules/.bin/eslint ./app/** --ext .jsx --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonreindl/anab.git"
  },
  "author": "Hashtagnull Team",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.9.1",
    "eslint": "4.4.0",
    "eslint-plugin-react": "7.2.1",
    "exports-loader": "^0.6.4",
    "express": "4.15.4",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^0.11.2",
    "imports-loader": "^0.7.1",
    "jest": "20.0.4",
    "lodash": "4.17.4",
    "node-sass": "4.5.3",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "^4.1.2",
    "react-test-renderer": "^15.6.1",
    "redux": "3.7.2",
    "redux-devtools": "3.4.0",
    "redux-thunk": "2.2.0",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "6.0.6",
    "scroll-behaviour": "^0.3.4",
    "style-loader": "0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.5.9",
    "webfontloader": "^1.6.28",
    "webpack": "3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "2.7.1",
    "whatwg-fetch": "2.0.3"
  },
  "homepage": "http://www.leonreindl.de",
  "jest": {
    "modulePaths": [
      "<rootDir>/app/"
    ],
    "notify": false,
    "collectCoverage": true,
    "verbose": true
  }
}
